/*----------------
Reset
-----------------*/

* {
  margin:0;
  padding:0;
}
*, *:before, *:after {
  box-sizing:border-box;
  -webkit-box-sizing:border-box;
  -moz-box-sizing:border-box;
}


body {
  margin:0;
  padding:0;
  color: {{ settings.body_text_color }};
}
body,
input,
textarea,
button,
select {
  font-family: {{ base_font.family }}, {{ base_font.fallback_families }};
  font-style: {{ base_font.style }};
  font-weight: {{ base_font.weight }};
}
:focus {
  outline:none;
}
ul, li {
  list-style:none;
  font-size:{{ settings.body_text_size | append: 'px'}};
}
a {
  text-decoration:none;
  color:{{ settings.primary_color }};
}
a:hover {
  color:{{ settings.link_hover_color }};
  text-decoration:none !important;
}
p {
  margin:0;
  padding:0;
  font-size:{{ settings.body_text_size | append: 'px'}};
}
.np {
  padding:0;
}
.pr45 {
  padding-right:45px;
}
.m15 { margin:15px;}
.mt10 { margin-top:10px; }
.mt15 { margin-top:15px; }
.mb15 { margin-bottom:15px; }
.mt20 { margin-top:20px; }
.mb20 { margin-bottom:20px; }
.mt30 { margin-top:30px; }
.mb30 { margin-bottom:30px !important; }
.pl10 { padding-left: 10px;}
.shopify-section:first-child .mt-30 {margin-top: -30px !important;}
@media (min-width: 769px) {
  .mb20d { margin-bottom:20px; }
}
.no-transition {
  -webkit-transition: height 0s;
  -moz-transition: height 0s;
  -ms-transition: height 0s;
  -o-transition: height 0s;
  transition: height 0s;
}
img.zoomImg {
  background:#fff;
}
.visually-hidden {
  position: absolute;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px; width: 1px;
  margin: -1px; padding: 0; border: 0;
}

/*----------------
Typography
-----------------*/


h1,h2,h3,h4,h5,h6 { 
  color:{{ settings.heading_text_color }}; 
  margin:0 0 13px 0; 
  font-family: {{ header_font.family }}, {{ header_font.fallback_families }};
  font-style: {{ header_font.style }};
  font-weight: {{ header_font.weight }};
}

h1 {
  font-size:34px;
  line-height:normal;
  margin:0;
  padding:0;
}
h2 {
  font-size:28px;
  line-height:normal;
  margin:0;
  padding:0;
}
h3 {
  font-size:22px;
  line-height:normal;
  margin:0;
  padding:0;
  font-weight:700;
}
h4 {
  font-size:18px;
  line-height:normal;
  margin:0;
  padding:0;
  font-weight:700;
}
h5 {
  font-size:14px;
  line-height:16px;
  margin:0;
  padding:0;
  /* text-transform:uppercase; */
  font-family: "IBM Plex Sans", sans-serif;
  font-weight:900;
}

/*----------------
Promo Bar
-----------------*/

.nav .open>a, .nav .open>a:focus, .nav .open>a:hover {
  background-color: transparent;
}

.nav>li>a:focus, .nav>li>a:hover {
  background-color: transparent;
}

.promo-bar {
  width:100%;
  text-align: center;
  background:{{ settings.promo_bar_bg_color }};
  padding:10px 0;
  color:{{ settings.promo_bar_text_color }};
}
.promo-bar a {
  color:{{ settings.promo_bar_text_color }} !important;
  border-bottom:1px dashed {{ settings.promo_bar_text_color }};
}
.promo-bar a:hover {
  color:{{ settings.link_hover_color }} !important;
  border-bottom:1px dashed {{ settings.link_hover_color }};
}

